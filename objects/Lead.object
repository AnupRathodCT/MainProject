    <validationRules>
        <fullName>Follow_Lead_Path</fullName>
        <active>true</active>
        <errorConditionFormula>(CASE( Status, &quot;Unqualified&quot;,1, &quot;New&quot;,2, &quot;Working&quot;,3, &quot;Nurturing&quot;,4, &quot;Converted&quot;,5,0) - CASE(PRIORVALUE(Status), &quot;Unqualified&quot;,1, &quot;New&quot;,2, &quot;Working&quot;,3, &quot;Nurturing&quot;,4, &quot;Converted&quot;,5,0)) &gt; 1</errorConditionFormula>
        <errorMessage>Make Sure Follow Status Field Step By Step</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_Number_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(REGEX(Phone, &quot;\\D*?(\\d\\D*?){10}&quot;))</errorConditionFormula>
        <errorMessage>Make Sure That Phone Number Has 10 Digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stop_Flow_Path_In_Backward</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
AND(
TEXT(Status) = &apos;Unqualified&apos;,
(TEXT(PRIORVALUE(Status)) = &apos;Nurturing&apos;)
),
FALSE,


CASE
( Status ,
&quot;Unqualified&quot;, 1,
&quot;New&quot;, 2,
&quot;Working&quot;, 3,
&quot;Nurturing&quot;, 4,
&quot;Converted&quot;,5,5)
&lt;
CASE
(PRIORVALUE( Status ) ,
&quot;Unqualified&quot;, 1,
&quot;New&quot;, 2,
&quot;Working&quot;, 3,
&quot;Nurturing&quot;, 4,
&quot;Converted&quot;,5,5))</errorConditionFormula>
        <errorMessage>You Can&apos;t Go Previous Stage Back.</errorMessage>
    </validationRules>
</CustomObject>
