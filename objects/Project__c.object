    <validationRules>
        <fullName>Cancelled_Project</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Project_Stage__c),
ISPICKVAL(PRIORVALUE(Project_Stage__c),&quot;Canceled&quot;)

)</errorConditionFormula>
        <errorDisplayField>Project_Stage__c</errorDisplayField>
        <errorMessage>Project Already Canceled. Can&apos;t Move To Any Other Stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delivered_Project</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Project_Stage__c),
ISPICKVAL(PRIORVALUE(Project_Stage__c),&quot;Delivered&quot;)

)</errorConditionFormula>
        <errorDisplayField>Project_Stage__c</errorDisplayField>
        <errorMessage>Project Delivered. Can&apos;t Move Back To Canceled Stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Employee_Assigned</fullName>
        <active>true</active>
        <errorConditionFormula>Employees_Assigned__c &gt; Employees_Required__c</errorConditionFormula>
        <errorDisplayField>Employees_Required__c</errorDisplayField>
        <errorMessage>Employees Assigned should not be greater than Employees Required on Project</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Forward_Path</fullName>
        <active>true</active>
        <errorConditionFormula>(CASE(Project_Stage__c, &quot;Draft&quot;,1, &quot;Canceled&quot;,2, &quot;Initiated&quot;,2,&quot;Requirements Gathering&quot;,3, &quot;Analysis&quot;,4,
&quot;Design&quot;,5, &quot;Development&quot;,6,&quot;Testing&quot;,7,&quot;UAT&quot;,8,&quot;Delivered&quot;,9,0) - CASE(PRIORVALUE(Project_Stage__c),&quot;Draft&quot;,1, &quot;Canceled&quot;,2, &quot;Initiated&quot;,2,&quot;Requirements Gathering&quot;,3, &quot;Analysis&quot;,4,
&quot;Design&quot;,5, &quot;Development&quot;,6,&quot;Testing&quot;,7,&quot;UAT&quot;,8,&quot;Delivered&quot;,9,0)) &gt; 1</errorConditionFormula>
        <errorDisplayField>Project_Stage__c</errorDisplayField>
        <errorMessage>Make Sure Follow Status Field Step By Step.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Project_Start_Date__c &lt; DATEVALUE(CreatedDate)</errorConditionFormula>
        <errorDisplayField>Project_Start_Date__c</errorDisplayField>
        <errorMessage>Project Start Date should Not Be a Past Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Backward_Flow_Path</fullName>
        <active>true</active>
        <errorConditionFormula>(CASE(Project_Stage__c, &quot;Draft&quot;,1, &quot;Canceled&quot;,11, &quot;Initiated&quot;,3,&quot;Requirements Gathering&quot;,4, &quot;Analysis&quot;,5,
&quot;Design&quot;,6, &quot;Development&quot;,7,&quot;Testing&quot;,8,&quot;UAT&quot;,9,&quot;Delivered&quot;,10,0) &lt; CASE(PRIORVALUE(Project_Stage__c),&quot;Draft&quot;,1, &quot;Canceled&quot;,2, &quot;Initiated&quot;,3,&quot;Requirements Gathering&quot;,4, &quot;Analysis&quot;,5,
&quot;Design&quot;,6, &quot;Development&quot;,7,&quot;Testing&quot;,8,&quot;UAT&quot;,9,&quot;Delivered&quot;,10,0))</errorConditionFormula>
        <errorMessage>You Can&apos;t Go Previous Stage Back.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
